{
  "name": "eleventy-blog-starter",
  "version": "1.0.0",
  "description": "A blog template using the Eleventy site generator.",
  "type": "module",
  "scripts": {
    "dev": "npx @11ty/eleventy --serve --port 3000 && npx tailwindcss -i ./src/css/styles.css -o ./public/style.out.css --watch",
    "build": "npx @tailwindcss/cli -i ./src/styles/style.css -o ./public/styles/style.css && eleventy"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@tailwindcss/cli": "^4.0.6",
    "@tailwindcss/typography": "^0.5.16",
    "@types/markdown-it": "^14.1.2",
    "cross-env": "^7.0.3",
    "luxon": "^3.5.0",
    "tailwindcss": "^4.0.6",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
  }
}
